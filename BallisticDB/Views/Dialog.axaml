<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:BallisticDB.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="150" 
        x:DataType="vm:MainViewModel"
        x:Class="BallisticDB.Views.Dialog"
        Icon="/Assets/avalonia-logo.ico"
        Height="150" Width="150"
        Title="Dialog"
        x:Name="dialog">
  <Design.DataContext>
    <vm:MainViewModel />
  </Design.DataContext>
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="auto"/>
      <RowDefinition Height="*"/>
      <RowDefinition Height="*"/>
      <RowDefinition Height="auto"/>
    </Grid.RowDefinitions>
    <TextBlock Grid.Row="0" Text="Hello"/>
    <Button Grid.Row="1" Content="Click Me" Command ="{Binding Save}" IsEnabled="True"/>
    <Button Grid.Row="2" Content="Click Me too"/>
  </Grid>

</Window>
